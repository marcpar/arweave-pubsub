{
  "name": "arweave_processor",
  "version": "1.0.0",
  "main": "src/index.ts",
  "type": "module",
  "license": "MIT",
  "devDependencies": {
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.6.5",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.19",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "scripts": {
    "dev": "ts-node --esm src/index.ts",
    "dev:watch": "nodemon -w src/**/* -e ts,js,json --exec 'yarn run dev'",
