    "clean": "rm -rf ./dist",
    "dispatch:storage-queue": "ts-node --esm src/bin/storage_queue_dispatch.ts"
  },
  "dependencies": {
    "@azure/storage-queue": "^12.10.0",
    "arweave": "^1.11.4",
    "dotenv": "^16.0.1"
  }
}
